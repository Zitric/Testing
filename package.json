{
    "name": "testing",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd": "^3.19.3",
        "axios": "^0.19.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "history": "^4.9.0",
        "moxios": "^0.4.0",
        "node-sass": "^4.12.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "redux": "^4.0.1",
        "redux-promise": "^0.6.0",
        "tv4": "^1.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "check-eslint-config": "eslint --print-config . | eslint-config-prettier-check",
        "check-code": "eslint src && prettier --check 'src/**/*.js'"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jest": "^22.7.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "husky": "^2.7.0",
        "lint-staged": "^9.1.0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run check-eslint-config && npm run check-code"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    }
}
